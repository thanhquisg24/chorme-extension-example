{
  "compilerOptions": {
    "esModuleInterop": true,
    "lib": ["esnext", "DOM", "dom.iterable", "WebWorker"],
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": false,
    "jsx": "react-jsx"
  },
  "include": ["src/**/*.tsx", "src/**/*.ts"],
  "exclude": ["node_modules"]
}
